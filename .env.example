# OpenWeather API key for weather data
OPENWEATHER_API_KEY=your_api_key_here

# Default ZIP code for weather lookup
DEFAULT_ZIP_CODE=12345

# LLM settings - Open-WebUI or OpenAI-compatible API
# For Docker networks, use the container name (e.g., http://open-webui:8080)
# Get your API key from Open-WebUI: Settings → Account → API Keys
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=dad-can-i-wear-this
OLLAMA_API_KEY=your-open-webui-api-key-here
OLLAMA_TIMEOUT=30.0